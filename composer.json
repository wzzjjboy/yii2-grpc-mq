{
  "name": "alan/yii2-grpc-mq",
  "description": "yii2 framwork use message queue with grpc(swoole)",
  "type": "library",
  "version": "1.0.2",
  "authors": [
    {
      "name": "alan",
      "email": "alan.wang@wetax.com.cn"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.22",
    "yiisoft/yii2-redis": "^2.0",
    "swoole/grpc":"dev-master",
    "ext-json": "*",
    "ext-swoole":"*",
    "ext-pcntl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "<7",
    "yiisoft/yii2-dev": "~2.0.14",
    "eaglewu/swoole-ide-helper": "dev-master"
  },
  "autoload": {
    "psr-4": {"alan\\yii2_grpc_mq\\": "src/"},
    "classmap": [
      "src/GPBMetadata/",
      "src/MqManage/"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  ]
}
